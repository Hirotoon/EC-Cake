<div class="col-xs-2"></div>
<div class="col-xs-8">
	<p style="font-size:25px;margin-top:100px;"><font style="background-color: silver;">会員一覧<font></p>
	<table class="table table-hover table-inverse">
		<thead>
		    <tr> 
		      <th>会員ID</th>
		      <th>氏名</th>
		      <th>メールアドレス</th>
		      <th>ステータス</th>
		    </tr>
		</thead>
		<% @end_users.each do |end_user| %>
			  <tbody>
				<tr>
					<td><%= end_user.id %></td>
					<td><%= end_user.last_name %><%= end_user.first_name %></td>
					<td><%= end_user.email %></td>
					<td>
						<% if end_user.is_active_member_status == true %>
							有効
						<% else %>
							退会済み
						<% end %>
					</td>
				</tr>
			  </tbody>
			 <% end %>
	</table>
</div>
<div class="col-xs-2"></div>
